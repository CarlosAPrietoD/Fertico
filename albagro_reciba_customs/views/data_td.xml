<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="cycle_form_view">
            <field name="name">reciba.datatd.form</field>
            <field name="model">reciba.datatd</field>
            <field name="arch" type="xml">
                <form string="Data TD Form">
                    <sheet>
                        <group>
                            <field name="name" readonly="True"/>
                            <field name="product_id"/>
                            <field name="condition"/>
                            <field name="percentage"/>
                            <field name="discount"/>
                            <field name="complement"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>



        <record model="ir.ui.view" id="td_tree_view">
            <field name="name">reciba.datatd.tree</field>
            <field name="model">reciba.datatd</field>
            <field name="arch" type="xml">
                <tree string="Data TD Tree">
                    <field name="name" readonly="True"/>
                    <field name="product_id"/>
                    <field name="condition"/>
                    <field name="percentage"/>
                    <field name="discount"/>
                    <field name="complement"/>
                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="td_action">
            <field name="name">Data TD</field>
            <field name="res_model">reciba.datatd</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">Create the first data
                </p>
            </field>
        </record>

        <menuitem id="menu_td" name="Data TD" parent="albagro_reciba_customs.menu_reciba" action="td_action" sequence="4"/>

    </data>
</odoo>