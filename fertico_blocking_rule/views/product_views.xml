<?xml version="1.0" ?>
<odoo>

    <record id="product_template_view_form_inherit_fertico" model="ir.ui.view">
        <field name="name">product.template.view.form.inherit.fertico</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_standard_price'][last()]" position="inside">
                <field name="is_sale_high_cost"/>
            </xpath>
        </field>
    </record>

</odoo>
