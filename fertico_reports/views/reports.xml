<odoo>
        <!--Formamos el template y nos apoyamos de las etiquetas de qweb para acceder a los datos que recibe desde el modelo-->
        <template id="report_contact">
            <t t-call="web.html_container">
                <t t-call="web.internal_layout">
                    <!--en el foreach recuperamos el dato guardado en la variable docs, en este caso un arreglo de objetos-->
                    <table t-foreach="docs" t-as="partner" class="table table-condensed" style="font-size: 15px; height: 60px;">
                        <tr><th colspan="10" style="padding: 5px; border: 0;"><t t-esc="partner['name']"/></th></tr>
                        <tr>
                            <th style="padding-top: 0; padding-bottom: 0; border-bottom: gray 1px solid;"></th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">JRNL</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Account</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Ref</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Due date</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Matching number</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Initial balance</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Debit</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Credit</th>
                            <th style="padding-top: 0; padding-bottom: 0; color: rgb(88, 88, 88); border-bottom: gray 1px solid;">Balance</th>
                        </tr>
                        <tr t-foreach="partner['moves']" t-as="move" t-if="move.debit != 0">
                            <!--con t-esc obtenemos los datos del objeto actual y accedemos al dato específico que queremos-->
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.date"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.move_id.name"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.account_id.code"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.ref"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.date_maturity"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);">0</td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.debit"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);"><t t-esc="move.credit"/></td>
                            <td style="padding-top: 0; padding-bottom: 0; color: rgb(117, 117, 117);">0</td>
                        </tr>

                    </table>
                </t>
            </t>
        </template>
        
        <!--
            En el reporte damos el modelo base que se va a tomar para el reporte o donde el boton aparecerá
            en el atributo name vamos a apuntar hacie el id del template teniendo en cuenta el modulo 
        -->
        <report id="report_custom"
        string="Reporte contable"
        model="res.partner"
        report_type="qweb-html"
        name="fertico_reports.report_contact"/>

    
</odoo>